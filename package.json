{
  "name": "koa-typescript-starter",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "CC0-1.0",
  "scripts": {
    "build-server": "tslint --project . && tsc",
    "watch-server": "cross-env NODE_ENV=development nodemon --watch src/**/* -e ts,tsx --exec 'ts-node src/server/server.ts'",
    "ts-node-dev": "cross-env NODE_ENV=development ts-node-dev --respawn --no-deps src/server/server.ts"
  },
  "devDependencies": {
    "@types/koa": "2.0.39",
    "@types/koa-router": "7.0.23",
    "@types/koa-session": "5.10.0",
    "@types/koa-static": "4.0.1",
    "@types/koa-views": "2.0.3",
    "@types/koa-websocket": "5.0.3",
    "cross-env": "5.2.0",
    "koa-views": "6.2.0",
    "nodemon": "1.19.0",
    "pug": "2.0.3",
    "ts-node": "3.3.0",
    "ts-node-dev": "1.0.0-pre.39",
    "tslint": "5.7.0",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "koa": "2.3.0",
    "koa-router": "7.2.1",
    "koa-session": "5.11.0",
    "koa-static": "5.0.0",
    "koa-websocket": "5.0.1"
  }
}
